{
  "name": "clientportal-settings",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vue-tsc --noEmit && NODE_ENV=development vite",
    "staging": "rimraf dist && vue-tsc --noEmit && NODE_ENV=staging vite",
    "build": "rimraf dist && vue-tsc --noEmit && NODE_ENV=production vite build && if [ -n \"$version\" ]; then mv ./dist/manifest.json ./dist/manifest-${version}.json && mv ./dist/clientclub-sw.js ./dist/clientclub-${version}-sw.js && mv ./dist/main-sw.js ./dist/main-${version}-sw.js && mv ./dist/firebase-messaging-sw.js ./dist/firebase-messaging-${version}-sw.js; fi",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "staging-build": "rimraf dist && vue-tsc --noEmit && NODE_ENV=staging vite build && if [ -n \"$version\" ]; then mv ./dist/manifest.json ./dist/manifest-${version}.json && mv ./dist/clientclub-sw.js ./dist/clientclub-${version}-sw.js && mv ./dist/main-sw.js ./dist/main-${version}-sw.js && mv ./dist/firebase-messaging-sw.js ./dist/firebase-messaging-${version}-sw.js; fi",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@gohighlevel/clientportal-core": "0.2.1",
    "@gohighlevel/ghl-icons": "^1.0.5",
    "@gohighlevel/ghl-ui": "^0.3.43",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.1",
    "cheerio": "^1.0.0-rc.12",
    "firebase": "^8.2.1",
    "js-cookie": "^3.0.5",
    "libphonenumber-js": "^1.10.34",
    "lodash.debounce": "^4.0.8",
    "moment-timezone": "^0.5.43",
    "pinia": "^2.0.22",
    "postcss": "^8.4.18",
    "postscribe": "^2.0.8",
    "tailwindcss": "^3.3.1",
    "vue": "^3.2.45",
    "vue-gtag": "^2.0.1",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.7",
    "vue-router": "^4.1.5",
    "vue3-otp-input": "0.4.1",
    "yup": "^1.1.1",
    "dayjs": "^1.11.7",
    "naive-ui": "^2.34.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@types/uuid": "^9.0.1",
    "@vue/cli-plugin-pwa": "~5.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-vue": "^9.8.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "typescript": "^4.6.4",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint",
      "yarn format"
    ]
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "1.22.19"
  }
}
